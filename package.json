{
    "name": "loproclang",
    "displayName": "Loproc",
    "description": "Loproc language support",
    "publisher": "Lucinotion",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.64.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "keywords": [
		"loproc",
		"loproclang",
        "lp",
		"highlight",
		"syntax"
	],
	"homepage": "https://github.com/Lucinotion/LoprocLang-vscode-support",
	"repository": {
		"type": "git",
		"url": "https://github.com/Lucinotion/LoprocLang-vscode-support"
	},
    "icon": "images/loproc.png",
    "contributes": {
        "languages": [{
            "id": "loproc",
            "aliases": ["Loproc", "loproc"],
            "extensions": [".lp"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "loproc",
            "scopeName": "source.lp",
            "path": "./syntaxes/loproc.tmLanguage.json"
        }]
    }
}